{
  "name": "etki/awful-sdk",
  "description": "A set of tools to simplify awful development",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Etki",
      "email": "etki@etki.name"
    }
  ],
  "bin": ["bin/asdk"],
  "autoload": {
    "psr-4": {
      "Etki\\AwfulSDK\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Etki\\AwfulSDK\\Tests\\": "tests/Suites",
      "Etki\\AwfulSDK\\Tests\\Support\\": "tests/Support"
    }
  },
  "require": {
    "symfony/yaml": "~2.5",
    "symfony/console": "~2.5",
    "twig/twig": "~1.16"
  },
  "require-dev": {
    "codeception/codeception": "~2.0.5",
    "mikey179/vfsStream": "*",
    "composer/composer": "@dev"
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "Am i a dog?": "Course you are, you little puffy barker"
  }
}
